{
  "printWidth": 80,
  "arrowParens": "always",
  "singleQuote": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "importOrder": ["<THIRD_PARTY_MODULES>", "next(.*)/", "@/", "./"],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
